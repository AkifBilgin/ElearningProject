@model List<MyElearningProject.DAL.Entities.Instructor>
@{
    Layout = null;
}


@{
    Layout = null;
}
@foreach (var item in Model)
{


<div class="tab-pane" id="profile" role="tabpanel">
    <div class="card-block">
        <div class="row">
            <div class="col-md-4 col-xs-6 b-r">
                <strong>Name</strong>
                <br>
                <p class="text-muted">@item.Name @item.Surname</p>
            </div>
            <div class="col-md-4 col-xs-6 b-r">
                <strong>Fächer</strong>
                <br>
                <p class="text-muted">@item.Title</p>
            </div>
            <div class="col-md-4 col-xs-6 b-r">
                <strong>Email</strong>
                <br>
                <p class="text-muted">@item.Email</p>
            </div>
    
        </div>
        <hr>
        <p class="m-t-30">@item.Description</p>
        

    </div>
</div>
}