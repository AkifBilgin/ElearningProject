@model MyElearningProject.DAL.Entities.Instructor
@{
    ViewBag.Title = "AddInstructor";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}
<h3>Dozent hinzufügen</h3>
<form method="post">
    <br />
    <label>Name</label>
    @Html.TextBoxFor(x => x.Name, new { @class = "form-control" })
    <br />
    <label>Nachname</label>
    @Html.TextBoxFor(x => x.Surname, new { @class = "form-control" })
    <br />
    <label>Foto</label>
    @Html.TextBoxFor(x => x.ImageUrl, new { @class = "form-control" })
    <br /><br />
    <button class="btn btn-primary">Speichern</button>
</form>

